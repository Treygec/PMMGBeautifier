{
	"manifest_version": 2,
	"name": "PMMG Beautifier",
	"description": "Improve and tweak the interface elements of PrUn website",
	"version": "0.0.3",
	"permissions": [
			"https://apex.prosperousuniverse.com/*",
			"file://*/*"
	],
	"content_scripts": [
		{
			"matches": ["https://apex.prosperousuniverse.com/*", "file://*/*"],
			"js": ["dist/main.js"]
		}
	]
}
